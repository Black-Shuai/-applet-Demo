/* pages/completionStatus/completion.wxss */
.container {
  width: 100%;
  height: 100%;
  background: #fff;
  }
   
  .title {
  font-size: 35rpx;
  color: black;
  text-align: center;
  }


page{
  display: flex;
  flex-direction: column;
  height: 100%;
}
.navbar{
  flex: none;
  display: flex;
  background: #fff;
}
.navbar .item{
  position: relative;
  flex: auto;
  text-align: center;
  line-height: 80rpx;
}
.navbar .item.active{
  color: rgba(0, 174, 255, 0.685);
}
.navbar .item.active:after{
  content: "";
  display: block;
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 4rpx;
  background: rgba(0, 174, 255, 0.685);
}